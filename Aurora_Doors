function isEyesSpawned(): boolean
    local eyes = nil
    eyes = workspace:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uGXHPDnJblbBHyuwQeRtfRaJpKapvJiFpjrxvSitLqSXlYigHlDTvkiRXllcw=='))
    return eyes ~= nil
end

