	Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WpcunvHolgicHVgAZCOVyfvbfvCdszYMmruZEPZMtodLymkbKFOcrThQW50aUV5ZXM='),
	Function = function(callback)
		if callback then
            task.spawn(function()
                repeat task.wait()
					if isEyesSpawned() then
						remotesFolder.MotorReplication:FireServer(-649)
					end
                until (not AntiEyes.Enabled)
            end)
        end
    end,
})

