local imageId = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('idfmnSdgvYxETbtEAwLHoWwAaxNRdgDTcuAryoZzwZwwuNjxndZuUovaHR0cDovL3d3dy5yb2Jsb3guY29tL2Fzc2V0Lz9pZD01ODU3MjEzMDg0')
local currentTheme = colors.Blue
local arrayItems = {}
local arraylist = {
	Create = function(o)

		local item = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XaUCqCjfpdYMaTajTEbFVCdFhODfJtgxiaUdnITfjNGdEYkClKaEyhZVGV4dExhYmVs'),arrayListFrame)
		item.Text = o
		item.BackgroundTransparency = 0.3
		item.BorderSizePixel = 0
		item.BackgroundColor3 = Color3.fromRGB(0,0,0)
		item.Size = UDim2.new(0,0,0,0)
		item.TextSize = 12
		item.TextColor3 = Color3.fromRGB(255,255,255)
		--item.TextXAlignment = Enum.TextXAlignment.Right

		local glow = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IVFziCVQjOTHMwFSoZNgaXKsbumAlhTfBnejGAAPDfyvRuPHeaXwCpQSW1hZ2VMYWJlbA=='),item)
		glow.Size = UDim2.fromScale(4,4)
		glow.BackgroundTransparency = 1
		glow.Image = imageId
		glow.ImageTransparency = 0.3
		glow.Position = UDim2.fromScale(-1,-1.5)
		glow.ZIndex = -10

		local size = UDim2.new(0.01,game.TextService:GetTextSize(o,22,Enum.Font.SourceSans,Vector2.new(0,0)).X,0.033,0)

		if o == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('hAXSDTZJsOnhjMYOUOMzOyyeieDttGjdTxETWyUYpHaRcPKZBetcuGb') then
			size = UDim2.fromScale(0,0)
		end

		item.Size = size

		item.BackgroundTransparency = 0.6
		item.TextTransparency = 0

		table.insert(arrayItems,item)
		table.sort(arrayItems,function(a,b) return game.TextService:GetTextSize(a.Text..RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('yKFxGWsCwuchkNUOuOpXogyrIRgyCFgeHEodnGgzhhOwnoPAhuSDvcPICA='),30,Enum.Font.SourceSans,Vector2.new(0,0)).X > game.TextService:GetTextSize(b.Text..RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('FhinBubTHWtRxkbeDRfScLTfVcjdERpahZpBPhtLXuBvfRLWtoExPTSICA='),30,Enum.Font.SourceSans,Vector2.new(0,0)).X end)
		for i,v in ipairs(arrayItems) do
			v.LayoutOrder = i
		end

	end,
	Remove = function(o)
		table.sort(arrayItems,function(a,b) return game.TextService:GetTextSize(a.Text..RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('nkzesTASbsdciyJQvlRKlbJCAoAkNUCyAVZELDWnGMfeBBNFRwIJyUYICA='),30,Enum.Font.SourceSans,Vector2.new(0,0)).X > game.TextService:GetTextSize(b.Text..RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EwEOrkjhIVncfNnIiLIevamqMMRgwaxadsCEImTDRgHXDqrHzgoQfEiICA='),30,Enum.Font.SourceSans,Vector2.new(0,0)).X end)
		local c = 0
		for i,v in ipairs(arrayItems) do
			c += 1
			if (v.Text == o) then
				v:Destroy()
				table.remove(arrayItems,c)
			else
				v.LayoutOrder = i
			end
		end
	end,
}

arraylist.Create(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('zSifdlscXUpaIFTyCTERyGVuVaAmtrHfIrmxsFbQbcDAQVfaTEalkOG'))

task.spawn(function()
	local loops = 0
	repeat task.wait()
		loops += 1
		local count = 0

		local half = #arrayItems / 2
		local glowTemps = {}
		for i,v in pairs(arrayItems) do
			count += 1
			local formula = ((count - 1) / (#arrayItems - 1))
			local offset = math.sin(tick() * 2 + formula * 2 * math.pi) / 0.5
			game.TweenService:Create(v,TweenInfo.new(0.5),{
				TextColor3 = currentTheme.Main:Lerp(currentTheme.Second,math.clamp(offset + 0.5,0,1))
			}):Play()
			game.TweenService:Create(v.ImageLabel,TweenInfo.new(0.5),{
				ImageColor3 = currentTheme.Main:Lerp(currentTheme.Second,math.clamp(offset + 0.5,0,1))
			}):Play()
		end

		if loops > 1 then
			for i,v in pairs(glowTemps) do
				v:Destroy()
			end

			table.clear(glowTemps)
		end
	until false
end)


function NewTween(item, totime, toChange)
	TweenService:Create(item, totime, toChange):Play()
end

