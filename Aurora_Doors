local isFools = floor.Value == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('QXPXLjToHdqBPVbZsZFspgmsQLSuvyqUSEsohzaSYjwdNrQwDjGmdqzRm9vbHM=')
