type ESP = {
    Object: Instance,
    Text: string,
    Color: Color3,
    Offset: Vector3,
    IsEntity: boolean
}
-- #endregion --

message.update_message_with_progress(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('joOPkboLCuWWuPUkRIluXejmyvpwnbPNZvqvbLiqRvjVXmMFTiKGbHYW2NsdXRjaC5sdWFdOiBDcmVhdGluZyBmdW5jdGlvbnMuLi4='), 2)
-- #region Functions --
function distanceFromCharacter(position)
    if typeof(position) == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EMziJAXhUxVebPxjaGpEJorXnfObdkUMxYTQqCvrirGyjPyIwMBHHlHSW5zdGFuY2U=') then
        position = position:GetPivot().Position
    end

    if alive then
        return (rootPart.Position - position).Magnitude
    else
        return (camera.CFrame.Position - position).Magnitude
    end

    return 9e9
end

local function inCutscene(): boolean
    local dist = (camera.CFrame.Position - character.Head.Position).Magnitude
    return (dist > 1)
end

function isEntitySpawned(): boolean
    local entity = workspace:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('nAmfDSciRXkBiHXLRIlWVRaxluuVSPadaYPzMEWVCubSCmehnWyYXjFUnVzaE1vdmluZw==')) or workspace:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('byoAmtDeLzTauJnnEqnfEbxoLOpQjpbXtfVBkWdcIoVdZIvvdQsmQkzQW1idXNoTW92aW5n'))

    if entity then
        if not entity.PrimaryPart then 
            repeat
                task.wait()
            until entity.PrimaryPart or not entity:IsDescendantOf(workspace)
        end

        if entity and distanceFromCharacter(entity:GetPivot().Position) < 2000 then
            return true
        end
    end

    return false
end
--

function isEyesSpawned(): boolean
    local eyes = nil

    if not isBackdoor then
        eyes = workspace:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('GAnIyNRAeOSvYVoeNSDXocsFutIFexMHwdSmkghDcIvFsBSgEECihcZRXllcw=='))
    else
        eyes = workspace:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('aYWvDlIuPOPjEGwUQiWeajmSGRNHkUqtheugJxTDEkPSfMfjpskjXJLTG9va21hbg=='))
    end

    return eyes ~= nil
end

function enableBreaker(breaker, value)
    breaker:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KZTXxPxZfvksdDIEvaezVfIlowRYhjBSTGPFVkDkQeDMkYiVVzgcCNARW5hYmxlZA=='), value)

    if value then
        breaker:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('zfYYodTcbicztfixiYXenxSkTUFcyaxlbjXlrQAvSrQzWOowXBQrihmUHJpc21hdGljQ29uc3RyYWludA=='), true).TargetPosition = -0.2
        breaker.Light.Material = Enum.Material.Neon
        breaker.Light.Attachment.Spark:Emit(1)
        breaker.Sound.Pitch = 1.3
    else
        breaker:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('YTGHpQTGlvfdfCYspNydZkqnewZBuAtBenapELETpkzMcypcdMeIQWQUHJpc21hdGljQ29uc3RyYWludA=='), true).TargetPosition = 0.2
        breaker.Light.Material = Enum.Material.Glass
        breaker.Sound.Pitch = 1.2
    end

    breaker.Sound:Play()
end

function esp(params: ESP)
    local EspManager = {
        Type = params.Type or RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('DZumboxKXzrFEJptUaaOmrHAIwLtnfuEjJgGojRjhnqksrbskDgMtyqTm9UeXBl'),
        Object = params.Object,
        Text = params.Text or RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KgpAlwVGbWcAVRLXZMrOElmJzyMXHOXesHCuJhvEGYTsPqJrrfUOIrBTm8gVGV4dA=='),
        TextParent = params.TextParent or nil,
        Color = params.Color or Color3.new(0, 0, 0),

        Offset = params.Offset or Vector3.zero,
        IsEntity = params.IsEntity or false,

        rsConnection = nil
    }

    local tableIndex = #espTable[EspManager.Type] + 1

    local traceDrawing = Drawing.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fHkruQobdciSrtwjMjvzwNvMrcaDHfcDAuGLxPbKUAeQLzOrGFBMvazTGluZQ==')) do
        traceDrawing.Visible = false
        traceDrawing.Color = EspManager.Color
        traceDrawing.Thickness = 1
    end

    if EspManager.Object and EspManager.IsEntity and EspManager.Object.PrimaryPart.Transparency == 1 then
        EspManager.Object:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wBomciGsqvpycBpVXhSkAmUJqBxhdRAfZpuQGhvdPCJPpzWppPWSWHgT2xkVHJhbnNwYXJlbmN5'), EspManager.Object.PrimaryPart.Transparency)
        Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VMwzQzxniHwLMelsOEJkesRRNZNqpDsWhcOdJShIHJERTXwzeoAiadcSHVtYW5vaWQ='), EspManager.Object)
        EspManager.Object.PrimaryPart.Transparency = 0.99
    end

    local highlight = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XyTrkZplrOEBmEJNzyKFJXQajVIhoIcyVTrHheifAtBPcTLwYmGjzikSGlnaGxpZ2h0')) do
        highlight.Adornee = EspManager.Object
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        highlight.FillColor = EspManager.Color
        highlight.FillTransparency = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lQPYkaHBczWcnSUXNTSSMQeLvzecQNZSLsydELxRKuiAHUpaIgByrNLRVNQRmlsbFRyYW5zcGFyZW5jeQ==')].Value
        highlight.OutlineColor = EspManager.Color
        highlight.OutlineTransparency = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vdsCXBqqejmnpRRwzvPQFKqMZRsfvvBrMKQCuDxDBoamiTvWvZSsdAwRVNQT3V0bGluZVRyYW5zcGFyZW5jeQ==')].Value
        highlight.Parent = EspManager.Object
    end

    local billboardGui = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HkMvTSpGBUvcfjSOVLUtqwAOhjDvqNrjDsMYcppGUGbCnWpCUqrVwaiQmlsbGJvYXJkR3Vp')) do
        billboardGui.Adornee = EspManager.TextParent or EspManager.Object
		billboardGui.AlwaysOnTop = true
		billboardGui.ClipsDescendants = false
		billboardGui.Size = UDim2.new(0, 1, 0, 1)
		billboardGui.StudsOffset = EspManager.Offset
        billboardGui.Parent = EspManager.TextParent or EspManager.Object
	end

    local textLabel = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dJIapjNtVDsFmtRnBQnUFSAFZvDykJDxzUKRSACFMIPwYdUBWDefnZSVGV4dExhYmVs')) do
		textLabel.BackgroundTransparency = 1
		textLabel.Font = Enum.Font.Oswald
		textLabel.Size = UDim2.new(1, 0, 1, 0)
		textLabel.Text = EspManager.Text
		textLabel.TextColor3 = EspManager.Color
		textLabel.TextSize = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('edaczBctGKtKzvDNlLEqYzYNpoxrDOFnxnXkLFWEZDlLHJOhqzjriXLRVNQVGV4dFNpemU=')].Value
        textLabel.TextStrokeColor3 = Color3.new(0, 0, 0)
        textLabel.TextStrokeTransparency = 0.75
        textLabel.Parent = billboardGui
	end

    function EspManager:SetColor(newColor: Color3)
        EspManager.Color = newColor

        highlight.FillColor = newColor
        highlight.OutlineColor = newColor

        textLabel.TextColor3 = newColor

        if traceDrawing then
            traceDrawing.Color = newColor
        end
    end

    function EspManager.Delete()
        if EspManager.rsConnection then
            EspManager.rsConnection:Disconnect()
        end

        if EspManager.IsEntity and EspManager.Object and (EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('unSYcAxLikwPkRdzXNNWMZgomQovnMjcWgdpGrVxxYTIIbcEXNWxWfWTW9kZWw=')) and EspManager.Object.PrimaryPart) then
            EspManager.Object.PrimaryPart.Transparency = EspManager.Object:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RUjADnHmGLCBkEFUAoAAQhsbvbsYjQsGlGRLAkjFRAksucRKgtupJADT2xkVHJhbnNwYXJlbmN5'))
        end

        traceDrawing:Destroy()
        highlight:Destroy()
        billboardGui:Destroy()

        if espTable[EspManager.Type][tableIndex] then
            espTable[EspManager.Type][tableIndex] = nil
        end
    end

    EspManager.rsConnection = RunService.RenderStepped:Connect(function()
        if not EspManager.Object or not EspManager.Object:IsDescendantOf(workspace) or not (EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fXmSQZuFSTzyHzXfWxuaGnanhWhBkuULfSYuqDwvAiqJiDzeLjoBKyJTW9kZWw=')) and EspManager.Object:GetPivot().Position or EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('itZWxOjWtdrWDEJhdoGcGVBeIwVaULGSQYtXQQCkFORYlbaxcaRkxGIQmFzZVBhcnQ=')) and EspManager.Object.Position) then
            EspManager.Delete()
            return
        end

        highlight.FillTransparency = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TYoRqxexafHFKlLaMsmXFjAgrDhzfAxBuQtBXXFkfceHckxsmwrnOznRVNQRmlsbFRyYW5zcGFyZW5jeQ==')].Value
        highlight.OutlineTransparency = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uCMcggBZxXPazUXzXcebAhwVNpXzoucvGUkYtGPBTCxrPbXnThVJUPSRVNQT3V0bGluZVRyYW5zcGFyZW5jeQ==')].Value
        textLabel.TextSize = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('qqTJbBLdvEeLmKtyvxghRuftUACSDojbIfLtEHzyMVViKghBApRYdlgRVNQVGV4dFNpemU=')].Value
        
        if rawMainGame and rawMainGame:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RJlNIOPzynTcATLtiPpIJRqJwnQAZkLpfBCYAabaFbexBOUXfsRsLQpUHJvbXB0U2VydmljZQ==')) then 
            local promptHighlight = rawMainGame.PromptService.Highlight

            if promptHighlight and promptHighlight.Adornee and (promptHighlight.Adornee == EspManager.Object or promptHighlight.Adornee.Parent == EspManager.Object.Parent) then
                promptHighlight.Adornee = nil
            end
        end

        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TPlByqKlOABCZlnDMScffUrgsnuDINwppjsZkSAuYhvjSNftoxpxBbJRVNQU2hvd0Rpc3RhbmNl')].Value then
            textLabel.Text = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('UlyYhRHOqbmNScsVBWikSZimxKauHrHuXNMohpHkDxDsvrxAqZoRlCkJXNcblslc10='), EspManager.Text, math.ceil(distanceFromCharacter(EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CKHNbQMbrEBmgkmleBwrhNuygDVaruMMGlmpkSBITLMmPPMrPQBIRqxTW9kZWw=')) and EspManager.Object:GetPivot().Position or EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('SCAcMMbTezPNsumXgDEsoPTnOfqIItglUgyvALQPzJNCcDVwslpcTDMQmFzZVBhcnQ=')) and EspManager.Object.Position)))
        else
            textLabel.Text = EspManager.Text
        end
        
        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('tmvdlHhMIPPiIKwDchSAFCyDZMffJsKVecJbQAkNkGNLpsviQPULWZlRVNQU2hvd1RyYWNlcnM=')].Value then
            local vector, onScreen = camera:WorldToViewportPoint(EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('cQQQPywtdvogJgeLVhKHMOUHVFPRmPbkXVVyjluDwjZkxZbCZVrNDlzTW9kZWw=')) and EspManager.Object:GetPivot().Position or EspManager.Object:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CDkQMzJXkNxwChWTfWhzczbvRygmidiqLEldcVJYqlEwTwCxhyqWZnKQmFzZVBhcnQ=')) and EspManager.Object.Position)

            if onScreen then
                traceDrawing.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
                traceDrawing.To = Vector2.new(vector.X, vector.Y)
                traceDrawing.Visible = true
            else
                traceDrawing.Visible = false
            end
        else
            traceDrawing.Visible = false
        end
    end)

    espTable[EspManager.Type][tableIndex] = EspManager
    return EspManager
end

function addDoorEsp(room)
    local door = room:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZZQADfKHOfTilScJxBXJzvnqSznqyuGbXFiRABRNINaPuSGAIbDJqRZRG9vcg=='))
    local locked = room:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IjetPqdgsOxBBzhqksoBTHmISgAYPBjTGHyopsnMEkRbQRIREqCtBPWUmVxdWlyZXNLZXk='))

    local isLibrary = room.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NybIeciJJOqGnjkYmqJhOesFRGzDdXZcJpyUmIjqafQhcJCgraXymKwNDk=') or room.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WUxomBkrwTbFjBihYICOrWxFZCkdeqEOKcTIwsmjmbxpSduFJaMBQAPNTA=')

    if door and door:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('BGMCIeoVODlpMJXvGVBJlZGBxyNojkwGQNRwLJmUeLrvsxiUtbQjfNFT3BlbmVk')) ~= true then
        local doorEsp = esp({
            Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wqzOGqBNulsTHsISRruQAVlbSzBKViazkDiWXbvGZZQQgmFqrRCmvmPRG9vcg=='),
            Object = ((isHotel or isFools) and isLibrary) and door or door:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('iANzmFwfUZTdmwfQhNkmuPJjaJGaAdFeYcqixEIjnKPGMiJOUuBLQOVRG9vcg==')),
            Text = locked and string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NjdfSgGZFDjMMEQppYWayfdwybueOUlxtfVtzjrlTifqPUlRQBmOiYmRG9vciAlcyBbTG9ja2VkXQ=='), room.Name + 1) or string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KqQqIocjxKVtLgbizcHLSvzLHgrhrgjRdBCNvOQSYkvsUyuoawoQSyiRG9vciAlcw=='), room.Name + 1),
            Color = Color3.fromRGB(0, 0, 255)
        })

        door:GetAttributeChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('aiLZRkBINlgqMXhiBFcDvaxYdQQWhcGWQpruPwKfZGpimIpGTWJtmEYT3BlbmVk')):Connect(function()
            local value = door:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WKBQXaVHgZYJflxeVGTbZjUSPxnHHLUPvwXyfKPYuyNCypbdoEcKNKmT3BlbmVk'))
            if doorEsp and value then doorEsp.Delete() end
        end)
    end
end

function addEntityEsp(entity)
    local entityName = getEntityName(entity)
            
    local entityEsp = esp({
        Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PGnUgIoliXAfTxPtudFbGPfWCAhSRrVMSbNrGeZXtpPYbDTXtNZjdErRW50aXR5'),
        Object = entity,
        Text = entityName,
        TextParent = entity.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('cfLqHZlbqEpzFzScGXfjdgsCRfthmQHtGdoEQCvTKzEEwORRGKyekQdSmVmZlRoZUtpbGxlcg==') and entity.PrimaryPart or nil,
        Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('saVNcCqciYbLZMujEKWTfVNNphCBXJcgklvLvzljpaJFtXNwoWUXbhlRW50aXR5RVNQQ29sb3I=')].Color,
        Offset = Vector3.new(0, 4, 0),
        IsEntity = entity.Name ~= RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TiFlMTUmVixtrxYzxucINdIynfzBRVGbWeoJtkSlBqbBXodOBmCLazdSmVmZlRoZUtpbGxlcg==') and true or false
    })

    if entityName == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VbyWNApPdXecaJoLEKAcSNPiRITvbVYuUDJaNHqiPQcRcUIISSpfOgaRXllcw==') then
        entity.PrimaryPart:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZZUoGJeFlabroWjMpheNnOEAcLVRkvMMVgCChpAxXhDsCdFklOhVNAdQW1iaWVuY2U=')):GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NKczPbAkheDUWemEQBSBjwNXiJBGbvnFOlmgrJogbAAkWxLCyQawbTWUGxheWluZw==')):Connect(function()
            if not entity.PrimaryPart.Ambience.Playing then
                entityEsp.Delete()
            end
        end)

        task.delay(3, function()
            if not entity.PrimaryPart.Ambience.Playing then
                entityEsp.Delete()
            end
        end)
    end
end

function addObjectiveEsp(room)
    task.spawn(function()
        if not room:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('YVWVwPxapdoydZIupvVvkMlYHQGagEOIyZNPOfVmmhScadrPgjJKhsUQXNzZXRz'), 3) then return end

        if room:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XpfgIsNWDBDPbnjCjjHqIslAOYqWdyTQnhDgRTflCUyzfuYcTvFiIziUmVxdWlyZXNLZXk=')) then
            local key = room:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('AsZSLAvZNiwWGrFtIWMEgupABxAjSnkQSKEoHnsIOqSxfQsiBuZZPjSS2V5T2J0YWlu'), true)

            if key then
                esp({
                    Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fBrKvyluJMLvemFQfJJntmrmvPIsndsXhgnRlrCIVxtFvbXTsftmicwT2JqZWN0aXZl'),
                    Object = key,
                    Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vGOoSShFLfPLeXYQSceVnHkeVLJVLAcRMWWicnazfalJnbgrsuFIugpS2V5'),
                    Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uFXhLvXTVnHhXxxcFRRpqKDlKKKFvHIdaQklKaDIAqyOWZbhJIZjgbfT2JqZWN0aXZlRVNQQ29sb3I=')].Color
                })
            end
        end

        if room.Assets:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('gFQAaSgKmeMiCZlGtMOVqngdwvRrwTdhKfyNxHfPQYFWzkMXhwRMKsLTGV2ZXJGb3JHYXRl')) then
            local lever = room.Assets.LeverForGate

            local esp = esp({
                Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fYeQWIyNdnPFlEgLeTXnLtdNXgfxbjFsKQsqRfxPYDpwuThuqYNDVnlT2JqZWN0aXZl'),
                Object = lever,
                Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lxBixbZyYUaSlaCdlZsREUtXdcoEytKLqHcfBVMmfnvSYevWZDbsLxPTGV2ZXI='),
                Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('pKaVhuobXsmCTmAxfGdEdBoHtqbeVaPovbHmrelOqmVYMAVepwxsdCnT2JqZWN0aXZlRVNQQ29sb3I=')].Color
            })

            lever.PrimaryPart:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TFSYFKYrcjTpNrdehtJGIrTBaCHvoBNJkShcOXoNATjflEEIgstkQjGU291bmRUb1BsYXk=')).Played:Connect(function()
                esp.Delete()
            end)
        elseif room.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VRsBksaMcmBgwmkTOJJnMCUJhsIUFYhTQMWswrJggLOYkquRWVlTPMuMTAw') then
            local key = room.Assets:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OqkXfzeIaMHAuokyEngWTUKSghkjOmNOYXGhKvGxelfiWfDXhEzLhmTRWxlY3RyaWNhbEtleU9idGFpbg=='), 5)
            if key then
                esp({
                    Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XWENmoSNElzaviTQeIfYJiMWMplKBZoIIFVRXTKbpUHVyarezGAuWSDT2JqZWN0aXZl'),
                    Object = key,
                    Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('BQDJGYQWmNCkrZFVufMEhNfbJdPrOKmHSLacrZkzFmMXyypGXfrJUiRS2V5'),
                    Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('xzlMQbXgvANVRptspBEYRJMSTxtlgsnGUopcOAqZzaYkimORKEXHAyiT2JqZWN0aXZlRVNQQ29sb3I=')].Color
                })
            end
        end
    end)
end

function addItemEsp(item, drop)
    local itemName = itemsTable.Names[item.Name] or item.Name

    esp({
        Type = drop and RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('rUhuXfnIGNrCsHHGuzWItAohGTfAKFoAgxxvHMtsFYFpAYsQHdIRmKgSXRlbURyb3A=') or RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XMMQiHRPNAOXQNnQqLxqwWORHQikJrLZDIhXblzeTGWzIlvKoCPfJHISXRlbQ=='),
        Object = item,
        Text = itemName,
        Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ermoegTWSklyFBmdhZWewSMTiNCzfbYwZfPgoNRaywqseeVeceZNUCRSXRlbUVTUENvbG9y')].Color
    })
end

function addGoldEsp(gold)
    esp({
        Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lxVrWgTjBhyDsrtUmhwjnQkmjTkOgQOBNNXeUAiEuCzfwvtztpEifvaR29sZA=='),
        Object = gold,
        Text = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('QcTRVyudHKUmjBpPzaDgNuQFsxdEwwLMUhBmpiQClMisHsGalqtPfueR29sZCBQaWxlIFslc10='), gold:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oeUdacNjksOBvPNTPoPEDRBnLTPtltkYFmmBRlnYVCGahJEPtILhbaaR29sZFZhbHVl'))),
        Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('xthjItscwdepZJlUCGJakNUkrcsycKYQvfZOXzStKusuJYMweRUfWpDR29sZEVTUENvbG9y')].Color
    })
end

function addPlayerEsp(player)
    if player.Character.Humanoid.Health == 0 then return end

    local playerEsp = esp({
        Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('DUuHjbSYdmCAsBSwuOJkLMGrPjLLaUIKLjMcOpcylJThlecyivhYAhBUGxheWVy'),
        Object = player.Character,
        Text = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NSTQLChPZZHaGxyRZdvbsFGOWNpYMLNilWOtFMbFuczWRnOCisiPSJeJXMgWyVzXQ=='), player.DisplayName, player.Character.Humanoid.Health),
        TextParent = player.Character:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RYcfWHzFXIZOYPvufsfTyUhViGaqmRdgrQFKOLDOYqVuWqjQoGulYXGSHVtYW5vaWRSb290UGFydA==')),
        Color = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('kpqJsehEyUQribkfzlkBIzAHPWxIjYduRqEMOwECyXTTcssvqLQhUyjUGxheWVyRVNQQ29sb3I=')].Color,
    })

    player.Character.Humanoid.HealthChanged:Connect(function(newHealth)
        if newHealth > 0 then
            playerEsp.Text = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KYJdilgdhHciKFHwrgTtBhFqVVtvHfQwYuZIrissrjGNBTHkDMzZEKqJXMgWyVzXQ=='), player.DisplayName, newHealth)
        else
            playerEsp.Delete()
        end
    end)
end

function addRoomEsp(room)
    task.spawn(function()
        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MtPfaTCrlrZymFRkzoeLqIBEwfQoLPVcHaNQmZReXpKuVhCTFGpIIkGRVNQV2hhdA==')].Value.Door then
            addDoorEsp(room)
        end

        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('aOkPqYfpOhAIUaYMekVhJQxKaLpiDKsemBMomNRWOFRmAtgqVZwDIlzRVNQV2hhdA==')].Value.Objective then
            task.delay(room.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VuIwDEJRiZuMabQGgrUGEDMFGYxVNnNVCAzsXxZxwpMGwDIGpqMHbWWNTA=') and 3 or 1, addObjectiveEsp, room)
        end
    end)
end

function addRoomConnection(room)
    room.DescendantAdded:Connect(function(child)
        if child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('hVOnedXHOGLAdzgwBtLvlygsRstuyZHCZjsnCfNbYhsPgtLNovcyeLtQmFzZVBhcnQ=')) and (child.Parent and child.Parent.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('UbMCkaboSUnPtNuHJeATersQssUOXdzPEIQOOEWWpRZJelbSOLkihruVHJpZ2dlckV2ZW50Q29sbGlzaW9u') and child.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EsIUgVRwvtHsbCUpNVkaDMNKVWUIAhyHhVmKChsZpeEMZPGuXyBBWXIQ29sbGlzaW9u')) then
            if #Players:GetPlayers() > 1 and Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('miXqZthlXEVVFbnHDYhXiPXaWYfkoJViDSORdwxmyYTkvzbVioXNHqFRkVBbnRpU2Vlaw==')].Value and rootPart then
                local currentRoom = latestRoom.Value + 1

                task.spawn(function()
                    repeat
                        firetouchtransmitter(child, rootPart, 1)
                        task.wait()
                        firetouchtransmitter(child, rootPart, 0)
                    until not child or latestRoom.Value > currentRoom
                end)
            elseif Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RBrGPtxEurGyqyjBgfMiDdmDxvPoWWycpnYwczepxZVqkBBpxteDjNyQW50aVNlZWs=')].Value then
                child.CanTouch = false
            end
        end

        task.delay(0, function()
            if child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dXwbQUHjpFXZiGzXzrvSsncqapFFEQkyUZBXRxfCAJWLZmkLVORfQEQUHJveGltaXR5UHJvbXB0')) then
                if promptTable.Aura[child.Name] ~= nil and not child:FindFirstAncestor(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vjayTviBhPLYaGTVIUbAZbwSQzyduboyQeqLZFKAugFueDueqnbYAnPUGFkbG9jaw==')) and not (isFools and child:FindFirstAncestor(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('kfxchYopTpAqTIOpbqgcZbmcopWVcRIGhViVXsfEXWJwaLMIJYeVFJfS2V5T2J0YWluRmFrZQ=='))) and not (isRetro and child:FindFirstAncestor(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oQKENnDOtpEQaiLPObffWuUeorrgankzrRkqQwvFXpxYuCTJzmEMsrVUmV0cm9XYXJkcm9iZQ=='))) then
                    table.insert(promptTable.AuraPrompts, child)
                end
                if isFools and promptTable.AuraFools[child.Name] ~= nil then
                    table.insert(promptTable.AuraPrompts, child)
                end
            end
        end)

        task.delay(0.1, function()
            if child:IsDescendantOf(workspace) and child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PaMRpFAtGkgTVtTkSvdyUnBzpREzFZncRxpHoGvQwFSZDksdZeMKHBsUHJveGltaXR5UHJvbXB0')) then
                if not table.find(promptTable.Excluded, child.Name) then
                    if not child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('iaiCKrdVhlDzSqNKkiPkZOoHKZFDcebphHbZmyKhnisscDLxTEiuRfgT3JpZ2luYWxEaXN0YW5jZQ==')) then
                        child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OPURwxvhWaFpXlEgoLOtLZjiDNqnRRCBwDtQNuHYQyBzCvSHDaSzmonT3JpZ2luYWxEaXN0YW5jZQ=='), child.MaxActivationDistance)
                    end
                    if not child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wGyUBLDyOjdVzufyxjUxprmtEyUteGGogeCYFuJKkJaVgYXaSLNISeKT3JpZ2luYWxFbmFibGVk')) then
                        child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('SCXOnggqecPnPGygKCrwrDVXsbLFiysECZIcJUGjJAyZCkJwdvXvhEYT3JpZ2luYWxFbmFibGVk'), child.Enabled)
                    end
                    if not child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HaOtyDqhiVDVeohFCkTpjEnZQNgOVZQkwTAdYYjVlmzNPyJJwRNoSjkT3JpZ2luYWxDbGlw')) then
                        child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lSHbdGXlFeWVhzAFMbmgJWCbYlpMMuzJMnKBJlhTcJPYmEAiBINQkgDT3JpZ2luYWxDbGlw'), child.RequiresLineOfSight)
                    end
                    
                    child.MaxActivationDistance = child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('xdeNGgVGrGoHvzzIMEaicGlHwGIONcHFLRWWwxOxFFKJCrHyjYiqknNT3JpZ2luYWxEaXN0YW5jZQ==')) * Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WErBuODwqotnXZGNEERPEfewIxOGHLmMvaHFkvEswvjAmtyzwhEtOfDUHJvbXB0UmFuZ2VCb29zdA==')].Value
                    
                    if isFools and Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('xOZZafknMdBRaokfMYNEawtqUedmbNvHFBPyVtISVEssGmzPCToosSWSW5zdGFJbnRlcmFjdA==')].Value then
                        if not child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NykWVFIbZOUrosHFGpsZnaLYVQSKZtuAThTpXIXpXKunlHHgLqwXeDTT3JpZ2luYWxEdXJhdGlvbg==')) then
                            child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OirIfESCkUGAenDjKSUdkcdvbTEWWpfloMJUOcurDIdlyenDppYfjhLT3JpZ2luYWxEdXJhdGlvbg=='), child.HoldDuration)
                        end
    
                        child.HoldDuration = 0
                    end
    
                    if child:IsDescendantOf(workspace) and Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vdAJIZGbsEWiOAnCLdKMWsIKelHRXjvyFSqKsopKiuEcFocEILivsrFUHJvbXB0Q2xpcA==')].Value and (table.find(promptTable.Clip, child.Name) or table.find(promptTable.ClipObjects, child.Parent.Name)) then
                        child.Enabled = true
                        child.RequiresLineOfSight = false
        
                        if child.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('QlmaSkRDAJECnHRYMmkcmVgdpolbORuSPmErNNBQDGdZlTMuDVcKpXvTW9kdWxlUHJvbXB0') then
                            child:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('bRfZbZJzlbyHBggICSuSJeIKhpCVngMiTZVjpDFYqXjYehMHkPPRSWeRW5hYmxlZA==')):Connect(function()
                                if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('aQtkREWloWdpdAPwJkOxKUosmLgSLAvFsThUnUmpDtQdRKrioeAllOvUHJvbXB0Q2xpcA==')].Value then
                                    child.Enabled = true
                                end
                            end) 
                        end
                    end
                end
            end
    
            if inCutscene() and child.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('LCzWvkAgRoQbPIQqidmtwLvDZgFQnjbOYWeRmVJSattltMuMVVjKyoORWxldmF0b3JCcmVha2Vy') and Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('YXJsJjRyhkZYLNRZjSqUUSRpRTzzxZaobfKygCDppPXfDNmxovhudWoQXV0b0JyZWFrZXJCb3g=')].Value then
                local autoConnections = {}
                local using = false
    
                if not child:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IwMGCvkEjagblEySPqLszHiKpuDOWuTvAhzRAQCeUWUYaigZRYJOTbPRHJlYWRSZWFjdGlvbg==')) then
                    child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('jmRlxeirCrPvGuUpsGmsfEwFfdWnteaskOemOaefehcBJWzipLqkXBERHJlYWRSZWFjdGlvbg=='), true)
                    using = true
    
                    if not (child:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PkMHqgkqFgrbIuCGjxvYDDMcoDPPrwvciRLZWZNIfTWtoCCBHEASBAyU3VyZmFjZUd1aQ=='), 5) and child.SurfaceGui:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dIKWtiNlghugbgyXLhbeheJkYjCWjGCWZoWxNCGwqcNNzuESqUeUPsTRnJhbWU='), 5)) then return warn(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MblYMuoFQkCIQZAfIvqJBhtmahPxkYpUlFmahmWzjwHzCKJcWGmOYKNQ291bGQgbm90IGZpbmQgZWxldmF0b3IgYnJlYWtlciBndWk=')) end
                    local code = child.SurfaceGui.Frame:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MtuqekEdXECPSdIIPujbfxceHdKDfypzddgBeTLioaTgojTqehlgvIoQ29kZQ=='), 5)
    
                    local breakers = {}
                    for _, breaker in pairs(child:GetChildren()) do
                        if breaker.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('SUDhowTgfthtNOUhuYPvVVvquJkKaVRtxBLZEeKRcXyWbBRoGfFFrzKQnJlYWtlclN3aXRjaA==') then
                            local id = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OwDCYKHMDEYDiffUYLDYVRGppdUseHhLaQyCfyNdZvwNGJUeOIombxCJTAyZA=='), breaker:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PUscehgJtKJXICloJHNdyMpufmiqBlCzWcyhSlERWmAwUQOlobutVNpSUQ=')))
                            breakers[id] = breaker
                        end
                    end
    
                    if code and code:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fPAJPooyvUZUVYriUawScnlCaSHnhCSumXoaOJCCYSwVxCfZoBiQsCBRnJhbWU=')) then   
                        local correct = child.Box.Correct
                        local used = {}
                        
                        autoConnections[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WEloBexZwJahKrVwYlENfsPvITwPEZdPdFptiJBxadVyELyoFomaKlNUmVzZXQ=')] = correct:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('FyCefRbZqVKQQUCiLGpHNSYHUwirPetcrMZnFCBUOEZDfzLgUsiHNLoUGxheWluZw==')):Connect(function()
                            if correct.Playing then
                                table.clear(used)
                            end
                        end)
    
                        autoConnections[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OEFOteAkbppPCjeKiduXrdTKMeaBoAPfGcjPHooKmOyaQmudcgCOEzcQ29kZQ==')] = code:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('cTohQVBlssqalrhBezkYZpWyYmUiWssfsAgSRlzTvmnEijImaIlHaLlVGV4dA==')):Connect(function()
                            task.wait(0.1)
                            local newCode = code.Text
                            local isEnabled = code.Frame.BackgroundTransparency == 0
    
                            local breaker = breakers[newCode]
    
                            if newCode == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('eixrlUfhlGDDgAxPaktDdnoMuSlWAaYkDjCAiKbGNehjLcMevNgkDeKPz8=') and #used == 9 then
                                for i = 1, 10 do
                                    local id = string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('pYvtStaCPNFPmbgprLdrlxtkAydAghHBZHJVPpXibGhDexFrTYwHJPoJTAyZA=='), i)
    
                                    if not table.find(used, id) then
                                        breaker = breakers[id]
                                    end
                                end
                            end
    
                            if breaker then
                                table.insert(used, newCode)
                                if breaker:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('cwGrdBXyNcjVEqozghbswJRmBVCqVibuciRnlmcWhaXpYZALscRLUwCRW5hYmxlZA==')) ~= isEnabled then
                                    enableBreaker(breaker, isEnabled)
                                end
                            end
                        end)
                    end
    
                    repeat
                        task.wait()
                    until not child or not Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('rVMbWhWHztOYlqsQawrPWUHGzReVIftMGoxghlQYDgcwpuIccLLswcfQXV0b0JyZWFrZXJCb3g=')].Value or not using
    
                    if child then child:SetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('haVKKBuhAuHMPcCTmfVTGvLymqJdowrCVecguLvHRWiebNzcueXgABURHJlYWRSZWFjdGlvbg=='), nil) end
                end
    
                for _, connection in pairs(autoConnections) do
                    connection:Disconnect()
                end
            end

