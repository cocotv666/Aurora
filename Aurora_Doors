function library:Notification(Description, Time)
	spawn(function()
		local newNotif = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PPoqYbWeiFztwVXfXXEyQaRFdEjpsAwimUIEgGGpFfthsQHFrSVfxFaVGV4dExhYmVs'), NOTIFY_FRAME)
		newNotif.Size = UDim2.fromScale(0.9, 0.1)
		newNotif.Position = UDim2.fromScale(0, 0)
		newNotif.BorderSizePixel = 0
		newNotif.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		newNotif.BackgroundTransparency = 0.4
		newNotif.TextColor3 = Color3.fromRGB(255,255,255)
		newNotif.TextScaled = true
		newNotif.Text = Description

		local Box = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('gaoFejBMRWYnzSNDyTrmfkuIYUYiMMdhuiVCeNtsTggtoMAstjAXTlkRnJhbWU='), newNotif)
		Box.Position = UDim2.fromScale(-0.05, 0)
		Box.Size = UDim2.fromScale(0.05, 1)
		Box.BorderSizePixel = 0
		Box.BackgroundColor3 = library.Color

		task.delay(Time, function()
			NewTween(newNotif, TweenInfo.new(1), {Transparency = 1})
			NewTween(Box, TweenInfo.new(1), {Transparency = 1})
		end)
	end)
end

local NotificationHolder = loadstring(game:HttpGet(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CWpaxpFfwBiJlgwEloVWIYmQpGyBqyPkFChsXzBACbcFUdBHShVVMXwaHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL0JvY3VzTHVrZS9VSS9tYWluL1NUWC9Nb2R1bGUuTHVh')))()
local Notification = loadstring(game:HttpGet(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('qZfhPZIrQILteKfkQbrkbueXTssiLhvIAbTmRhqlmuhncZBhbfzoQAFaHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL0JvY3VzTHVrZS9VSS9tYWluL1NUWC9DbGllbnQuTHVh')))()

function CreateNotification(Title, Duration, Message)
    Notification:Notify(
        {Title = Title, Description = Message},
        {OutlineColor = Color3.fromRGB(0, 191, 255), Time = Duration, Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ICFQlZyzIWOlSfllYHllBARysSYPZOpcjyhswrVKrwVnbtGfPVzBXHNaW1hZ2U=')},
        {Image = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JGDSGulsEgnrpycKRNpoWKEJBtQoooncKjDoNqdZLiwabOTdkEOgogTaHR0cDovL3d3dy5yb2Jsb3guY29tL2Fzc2V0Lz9pZD02MDIzNDI2OTIz'), ImageColor = Color3.fromRGB(0, 191, 255)}
    )
end

function CreateNotification2(Title, Duration, Message)
    Notification:Notify(
        {Title = Title, Description = Message},
        {OutlineColor = Color3.fromRGB(255, 255, 0), Time = Duration, Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MVBkObRCIQaPRDLQqfZTSMHedhpcUxmgmRmxkuBoyKJJjpwlPDxTgVvaW1hZ2U=')},
        {Image = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('rYyKIwHNXYxXEUfzZbJvzWDyUePaeZKheiapaXUyfUaVxSiWrbnBtNWaHR0cDovL3d3dy5yb2Jsb3guY29tL2Fzc2V0Lz9pZD02MDMxMDcxMDUz'), ImageColor = Color3.fromRGB(255, 255, 0)}
    )
end

function CreateNotificationStaffDetector(Title, Duration, Message)
    sound = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('qzTKSriGXfufNpXmeRkkYgHLEtFJEmzwweAxflKvbcUYHpUIGWDzldoU291bmQ='),workspace)
    sound.Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oiihVEgdcqigYAjTTaTFnVZsDkTqzotGBGKyNmSaxBIaXPMgBBZjBvATm9vb29vb29vb29vb29vb290IG5vb29vb29vb29vb29vb29vb29vb29vb290')
    sound.SoundId = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wRgCsUXqJnlvWHNsJljyKkOZmzrvIPXTmqaibrEhFbisYdyuPPPqcqwcmJ4YXNzZXRpZDovLzczOTY3NjI3MDg=')
    sound:Play()

    Notification:Notify(
        {Title = Title, Description = Message},
        {OutlineColor = Color3.fromRGB(255, 0, 0), Time = Duration, Type = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('tKyUECyHKxODcFipFmIyzUBgoMoNVjeQjvWyjHvtFdAYEuOmYNOVIANaW1hZ2U=')},
        {Image = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MYEGdhrelvBwtuvodUccHlrEROXoJcgnpXOwHozDOHRRgsitwNCEOGwaHR0cDovL3d3dy5yb2Jsb3guY29tL2Fzc2V0Lz9pZD02MDMxMDcxMDUz'), ImageColor = Color3.fromRGB(255, 84, 84)}
    )
end

library.NewWindow = function(name)
	local textlabel = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('NbDXXOEPbPwuYrvtdLbNstTZUvpYCOfhqxCHrFketWERiYEXRdqvdIsVGV4dExhYmVs'), ScreenGui)
	textlabel.Position = UDim2.fromScale(0.1 + (library.WindowCount / 8), 0.1)
	textlabel.Size = UDim2.fromScale(0.1, 0.0425)
	textlabel.Text = name
	textlabel.Name = name
	textlabel.BorderSizePixel = 1
	textlabel.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	textlabel.BackgroundTransparency = 0.2
	textlabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	textlabel.BorderColor3 = Color3.fromRGB(50, 50, 50)
	textlabel.ZIndex = 2

	local modules = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ArwpkhFVelqAwhMKdvNSGbmUMcysWhmrDUAwPfFKYCBUVamCrCpxuQuRnJhbWU='), textlabel)
	modules.Size = UDim2.fromScale(1, 5)
	modules.Position = UDim2.fromScale(0, 1)
	modules.BackgroundTransparency = 1
	modules.BorderSizePixel = 0

	local sortmodules = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('rQrxdLQIfFmsSsFYwWwTpjmQgqnFdXBNieqDxPQwVZnjbSRcHrBEoEBVUlMaXN0TGF5b3V0'), modules)
	sortmodules.SortOrder = Enum.SortOrder.Name

	UserInputService.InputBegan:Connect(function(k, g)
		if g then return end
		if k == nil then return end
		if k.KeyCode == library.KeyBind then
			textlabel.Visible = not textlabel.Visible
		end
	end)

	library.WindowCount += 1

	local lib = {}

	lib.NewButton = function(Table)
		library.Modules[Table.Name] = {
			Keybind = Table.Keybind,
		}

		if config.Buttons[Table.Name] == nil then
			config.Buttons[Table.Name] = {
				Enabled = false,
			}
		else
			saveConfig()
		end

		if config.Keybinds[Table.Name] == nil then
			config.Keybinds[Table.Name] = tostring(Table.Keybind):split(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PPqpsYtQYBpxUgyZKmYqcKyasXFOujDYzCGgLWaMNKYjhgmwqCcvbaNLg=='))[3] or RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZKfmKmrJsNKbNrWvgxJEdLuqmeoQiGttkfZeenWcmPBawdAtbQNeFvrVW5rbm93bg==')
		end

		library.Modules[Table.Name].Keybind = Enum.KeyCode[config.Keybinds[Table.Name]]

		local enabled = false
		local textbutton = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('bHCifPEQATcrExHiBHFPUzMlNzgwhycCmLpyurRluniJSBitTsQivfmVGV4dEJ1dHRvbg=='), modules)
		textbutton.Size = UDim2.fromScale(1, 0.2)
		textbutton.Position = UDim2.fromScale(0, 0)
		textbutton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		textbutton.BackgroundTransparency = 0.3
		textbutton.BorderSizePixel = 1
		textbutton.BorderColor3 = Color3.fromRGB(70, 70, 70)
		textbutton.Text = Table.Name
		textbutton.Name = Table.Name
		textbutton.TextColor3 = Color3.fromRGB(255, 255, 255)

		local dropdown = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('GWRMdtjvubMOfHIIKObiAGRxMNCGGkKZfYhRsmcTfleMQQrjfQxIvooRnJhbWU='), textbutton)
		dropdown.Position = UDim2.fromScale(0, 1)
		dropdown.Size = UDim2.fromScale(1, 5)
		dropdown.BackgroundTransparency = 1
		dropdown.Visible = false

		local dropdownsort = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EaZoeXDowomeDgVHnMSCbykUrRUCsTJrRuvqrHYpawPsqifHbozIIZgVUlMaXN0TGF5b3V0'), dropdown)
		dropdownsort.SortOrder = Enum.SortOrder.Name

		local lib2 = {}
		lib2.Enabled = false

		lib2.ToggleButton = function(v)
			if v then
				enabled = true
			else
				enabled = not enabled
			end

			if enabled then
				arraylist.Create(Table.Name)
				CreateNotification(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('jMpxgpxKFykHdyXcYeuHwQQYHkIikOkVGgzJxjjNsBGWMNachWTCqSSQXVyb3Jh'), 5, Table.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CWCRLKBlVfmUWpBLjkAiCwKgUbCkasJcuPHxtpjGWncepFgLeBNBARgIGhhcyBiZWVuIGVuYWJsZWQ='))
			else
				arraylist.Remove(Table.Name)
				CreateNotification(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PyTjGgnFIOhHlrixhrRobEmbTaKstypFoJCFZSvTvmNBZeUtprbCEvVQXVyb3Jh'), 5, Table.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('sBkPBuqOQYeymdcOcjCNMaumAZeJdQkRiipoeLyTXBNPjtaDsJLvZGmIGhhcyBiZWVuIGRpc2FibGVk'))
			end

			lib2.Enabled = enabled
			task.spawn(function()
				task.delay(0.1, function()
					Table.Function(enabled)
				end)
			end)

			textbutton.BackgroundColor3 = (textbutton.BackgroundColor3 == Color3.fromRGB(60, 60, 60) and library.Color or Color3.fromRGB(60, 60, 60))
			config.Buttons[Table.Name].Enabled = enabled
			saveConfig()
		end

		local keybindButton = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('nNnLGMOPZEqwWviqarhGSKPPsAnFYNufnjhdMCTOVKrCYGmbwxDfFELVGV4dEJ1dHRvbg=='), dropdown)
        keybindButton.Size = UDim2.fromScale(1, 0.15)
        keybindButton.Position = UDim2.fromScale(0, 0)
        keybindButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        keybindButton.BackgroundTransparency = 0.3
        keybindButton.BorderSizePixel = 1
        keybindButton.BorderColor3 = Color3.fromRGB(70, 70, 70)
        keybindButton.Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('zjsivcztbvwbhwKDJdHBSPkADfdhvlZYuWlXDbothiERryZfZFPJjtXS2V5YmluZDog') .. config.Keybinds[Table.Name]
        keybindButton.TextColor3 = Color3.fromRGB(255, 255, 255)

        local keybindConnection
        keybindButton.MouseButton1Down:Connect(function()
            keybindConnection = game.UserInputService.InputBegan:Connect(function(input, gameProcessed)
                if gameProcessed then return end
                keybindButton.Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JAgfjlSKOTTYEBylYiZfHxghSgkkynqZaduZjxVirqXcrLzvixmKiKSS2V5YmluZDog') .. tostring(input.KeyCode):split(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('FDviFqRhwwdgbyunQMVdMEZvKbGAvbfPhPFMHdXiFyyCaHfKSyZddnjLg=='))[3]
                config.Keybinds[Table.Name] = tostring(input.KeyCode):split(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CeQFJwlXHlKYHGbobMunQaCjObLZNCoDtNUjdYrxvBHvmyYQppxSGNXLg=='))[3]
                library.Modules[Table.Name].Keybind = Enum.KeyCode[config.Keybinds[Table.Name]]
                task.wait(0.06)
                saveConfig()
                keybindConnection:Disconnect()
            end)
        end)

		lib2.NewToggle = function(v)
			local Enabled = false

			if config.Toggles[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wKkkeUToCkMWtNGRDSYdoqMmiXwvKOnieSXuMirNPVJKutmclPTabxHXw==') .. Table.Name] == nil then
				config.Toggles[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('URbwDIDXeSElMejswzNdUKOlJChrodwKRRKMKbDQAaVfkujlEAZfjvQXw==') .. Table.Name] = {Enabled = false}
			end

			local textbutton2 = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZEDuKpqNIfKbLAXLubGzJPcmWiRpqzOblgKpTpDBKCWROXxuunEHRJnVGV4dEJ1dHRvbg=='), dropdown)
			textbutton2.Size = UDim2.fromScale(1, 0.15)
			textbutton2.Position = UDim2.fromScale(0, 0)
			textbutton2.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
			textbutton2.BackgroundTransparency = 0.3
			textbutton2.BorderSizePixel = 1
			textbutton2.BorderColor3 = Color3.fromRGB(70, 70, 70)
			textbutton2.Text = v.Name
			textbutton2.Name = v.Name
			textbutton2.TextColor3 = Color3.fromRGB(255, 255, 255)

			local v2 = {}
			v2.Enabled = Enabled

			v2.ToggleButton = function(v3)
				if v3 then
					Enabled = v3
				else
					Enabled = not Enabled
				end
				v2.Enabled = Enabled
				task.spawn(function()
					v[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vIqZcVbxSqsIxntNvuxsNiLzslXpwojwwebbEVdpWHNOqdrtHqYWFhnRnVuY3Rpb24=')](Enabled)
				end)
				textbutton2.BackgroundColor3 = (textbutton2.BackgroundColor3 == Color3.fromRGB(60, 60, 60) and library.Color or Color3.fromRGB(60, 60, 60))
				config.Toggles[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('hHQYwkfuoNOTdgpnZwQZixsUQwghvgMqyYGZdwxTUkKeWjYXAXbCAxsXw==') .. Table.Name].Enabled = Enabled
				saveConfig()
			end

			textbutton2.MouseButton1Down:Connect(function()
				v2.ToggleButton()
			end)

			if config.Toggles[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('gFmBhPgXDuiRvlcoDDwmCyqwbdzbMRjTZXrMAsstDdiQzHOHHHfeRjmXw==') .. Table.Name].Enabled then
				v2.ToggleButton()
			end

			return v2
		end

		lib2.NewPicker = function(v)
			local Enabled = false

			if config.Options[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lLjzFYgwTvujMHKWABTcsAjxFIEqTvuQJlLTluveJdIEWFPMNtPVwbyXw==') .. Table.Name] == nil then
				config.Options[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dbXoVTmpgwZSqMMBtTAUrMvliORhAaFBuCnRkgwSesOsepFUDXsMBXHXw==') .. Table.Name] = {Option = v.Options[1]}
			end

			local textbutton2 = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('UjvCmesBTQdpUPhILFPbJncyirIELSRselriAFcchAACPOtaRdfRThmVGV4dEJ1dHRvbg=='), dropdown)
			textbutton2.Size = UDim2.fromScale(1, 0.15)
			textbutton2.Position = UDim2.fromScale(0, 0)
			textbutton2.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
			textbutton2.BackgroundTransparency = 0.3
			textbutton2.BorderSizePixel = 1
			textbutton2.BorderColor3 = Color3.fromRGB(70, 70, 70)
			textbutton2.Text = v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('lAtiSGLFxlDtGfxhgHNLTWeFGfFvWXQPmKMbqKRnhYqoBdgdaLMpjXkIC0g') .. v.Options[1]
			textbutton2.Name = v.Name
			textbutton2.TextColor3 = Color3.fromRGB(255, 255, 255)

			local v2 = {
				Option = v.Options[1]
			}

			local index = 0
			textbutton2.MouseButton1Down:Connect(function()
				index += 1

				if index > #v.Options then
					index = 1
				end

				v2.Option = v.Options[index]
				textbutton2.Text = v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('iPXmScwSxoLGVZFkPfxYfyhkAsWfbqIyJksznjdToxLGFZEDgtXqmCAIC0g') .. v2.Option

				config.Options[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('rSWsVIxgGybQGTBexFkJMcupxholjFbtXdXSGemxAhsLKHHdOSnWETyXw==') .. Table.Name].Option = v.Options[index]
				saveConfig()
			end)

			textbutton2.MouseButton2Down:Connect(function()
				index -= 1

				if index < 1 then
					index = #v.Options
				end

				v2.Option = v.Options[index]

				textbutton2.Text = v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IIlAbSsZLBiJGgCVtOHIAhUcIjFhTNMlwwyuFBAOwOBQSYnyUREtYQYIC0g') .. v2.Option
				config.Options[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('sZyJPcpEzhicTsUBSHReQAZXFxCZcsvDaMmNfonwCnBATOgJIJdlPffXw==') .. Table.Name].Option = v.Options[index]
				saveConfig()
			end)

			local option = config.Options[v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EALjIJgzyUwrEINkPdGvCPsKxSmnyzQmuUKRmptgNahTtrOYKnlbFXEXw==') .. Table.Name].Option
			v2.Option = option
			textbutton2.Text = v.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uldwpZrdnYujKjDdyBhKJYmfKkfzhVIcOBHWykCtWrvFDFURRQxLsDwIC0g') .. option

			return v2
		end

		textbutton.MouseButton1Down:Connect(function()
			lib2.ToggleButton()
		end)

		textbutton.MouseButton2Down:Connect(function()
			dropdown.Visible = not dropdown.Visible
			for i, v in pairs(modules:GetChildren()) do
				if v.Name == Table.Name then continue end
				if v:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oUAzMmKXxDKOEOLoCoWJGZfsJBhCOunmVfqGosbZphgsGRTRNYsWrbrVUlMaXN0TGF5b3V0')) then continue end
				v.Visible = not v.Visible
			end
		end)

		UserInputService.InputBegan:Connect(function(k, g)
			if g then return end
			if k == nil then return end
			if k.KeyCode == library.Modules[Table.Name].Keybind and k.KeyCode ~= Enum.KeyCode.Unknown then
				lib2.ToggleButton()
			end
		end)

		if config.Buttons[Table.Name].Enabled then
			lib2.ToggleButton()
		end

		return lib2
	end

	return lib
end


