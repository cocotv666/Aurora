function disableDupe(closet, value)
    local doorFake = closet:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('URiIIpwswNOGxzbWlNyRZWRaaTyQAEgVhZCgmpJypeeqFKoYpIJDBSvRG9vckZha2U='), 5)

    if doorFake then
        doorFake:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('bxHMoPWOouRuesdihEKWLLwcoBGZGUAEyEaoGGcoAGBBxebilsQsqASSGlkZGVu'), 5).CanTouch = not value
        local lock = doorFake:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('sCGalMxNpcAFUCUJGiahVpIetdObfDkNOehaFrcyXDLIaHqOdMNteiBTG9ja1BhcnQ='), 5)

        if lock and lock:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('BAxECBatAGAtzsRuQHxyAAIdcKfmpJvwqGpWrJOTPqLPWMaNxNiccXxVW5sb2NrUHJvbXB0')) then
            lock.UnlockPrompt.Enabled = not value
        end
    end
end

function ghostPart(part)
    if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MbnvDEJGnxpVleeiaqeBmxIIRSrwpcSlkmAmHGKaxdEaoUYFGpIDRAAR2hvc3RCb2R5')].Value then
        part.LocalTransparencyModifier = Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PoqCiVQwgBZZNuqaqYEDJzfDJRUHFczsGVTQdZiAaIaJEabfLZbhkNNQm9keVRyYW5zcGFyZW5jeQ==')].Value
    end
end

function ghostPlayer(character)
    for _, part in pairs(character:GetChildren()) do
        if part:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ddsaEQBwNStmmOBvuJwjvjKSuBCBkJOZBIrbYlLBCIbuQGqDUgJWUIfQmFzZVBhcnQ=')) and part.Name ~= RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('aSdqXQfqhAnuMMomdhbxabuikjkyyVmobsViDZqMjLFrxVCuTRmyDQqSHVtYW5vaWRSb290UGFydA==') then
            ghostPart(part)

            part:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OqBZfNPomkrRCfTXOrWcapEeOVKTwcXfQaUjACHHZzLSHLuRcZvGYqNTG9jYWxUcmFuc3BhcmVuY3lNb2RpZmllcg==')):Connect(function()
                ghostPart(part)
            end)
        end
    end
end

function setupCharacterConnection(newCharacter, reloading)
    if not newCharacter then return warn(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZeIUkZruXdwAesfQMwZPqERJsSCHHvWwLVWGHFEksSkzXsCDYLlBmFDSW52YWxpZCBjaGFyYWN0ZXI=')) end

    character = newCharacter

    local bodyVelocity = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MHOaAOyWwYCPpMUoHoZAUwPbksyQmevZOSLvYvvrXDfJePDyfcMdEIOQm9keVZlbG9jaXR5')) do
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    end

    objects.bodyVelocity = bodyVelocity

    task.spawn(ghostPlayer, character)
    connections[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('LQPgiRFAChtTtHSoLBtJqVcRaeowdTOeMiRFlpcueEbqJipIzXXSxRVQ2hhcmFjdGVyQ2hpbGRBZGRlZA==')] = character.ChildAdded:Connect(function(child)
        if child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('egjrDrjrxIOuDMXANdCzhmdUhVfpMJyHsnRRwMTWtVPpqPhcYtaNCXLQmFzZVBhcnQ=')) and child.Name ~= RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CQLNEzWWVRSIrKNqYNyaSeXNjYHsEjomzqrBtCKgixasSwVlZTCZTLvSHVtYW5vaWRSb290UGFydA==') then
            ghostPart(child)

            child:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('mpcLUYXiNfOoGEpxFwkjoSZHqySHPIybCtNFRQHAksuPhLIeAPQhCiOTG9jYWxUcmFuc3BhcmVuY3lNb2RpZmllcg==')):Connect(function()
                ghostPart(child)
            end)
        end

        if child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fJXICVzmbIPFzFNxdNqkyjpUifjhXGCIrjzxbvOVXXUWxUhgyTrrhdzVG9vbA==')) and child.Name:match(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vcDpWOowZHWnknaPSvmwYtNjXwgRdPMrRJPtqIkETiRZnbhsBPpLoquTGlicmFyeUhpbnRQYXBlcg==')) then
            task.wait()
            local code = table.concat(getPadlockCode(child))
            local output, count = code:gsub(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IyguVQjmWLlEhQKRjMqytrXDTKQvmWYXoHppiMEcnhlThNgggVJfefCXw=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oGdcroytnonZEOvoDZUmixXPTFoZBDEgbHwTMsRYACoEgWfQLdqhBSJeA=='))

            if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PbLULgLcqGvQbhgOmEWiJeRGErDDclVTAgNUZqhXdJahxHnmuDkmNAYQXV0b1BhZGxvY2s=')].Value and tonumber(code) then
                remotesFolder.PL:FireServer(code)
            end

            if count < 5 then
                if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dcfiplMqgILfZsYgGGKMktrnjAkiUZEVWvEZeciMcYUJCzcbAamVVZgTm90aWZ5UGFkbG9ja0NvZGU=')].Value then
                    Midnight:Notify(string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EnRLvpCjZotxelPLIieKcQXGncSgEiyayUwOkQJNLTLiHATRgxmspMSVGhlIHBhZGxvY2sgY29kZSBpczogJXM='), output))
                end
            end
        end
    end)

    connections[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vNLHatuMCkgWlqYPneTSJTKnivQPJUwWIsqHSeuOrrINVgtQDoTOHJsQ2hhcmFjdGVySGlkaW5n')] = character:GetAttributeChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('yKIRxRWDFQcVTgGQslWyhBceYmMXAipxUCaEQgpNBwvFhKmWuVCpLpsTGFzdEhpZA==')):Connect(function()
        lastHidden = character:GetAttribute(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('yTzIWfpzxtCZCMiLsQhBOHXVjdpaWnOzGxbNZoUmYxlXFtlDsANsueJSGlkaW5n')) and workspace:GetServerTimeNow() or 0
    end)

    humanoid = character:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KsPZDSMvwLUcAgLUvDOQSzYVHAdQSRNwXiAvZnpgkgHmvoEhSRisUWWSHVtYW5vaWQ='), 3)
    if humanoid then
        if humanoid.Health > 0 then
            task.delay(1, function()
                tracks.holdingObjTrack = humanoid:LoadAnimation(holdAnim)
                tracks.throwObjTrack = humanoid:LoadAnimation(throwAnim)
                tracks.twerkTrack = humanoid:LoadAnimation(twerkAnim)

                if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('pqTcbiFhgJBEAPmUNaSzccibMFsJrJsxewlhDCVvFlfgvpeQREKEttQVHdlcms=')].Value then
                    tracks.twerkTrack:Play()
                end

                
            end)
        end

        connections[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('FmmtgRIgIQTCQonrFXvzDdGQKzTqnclGAcHOVMIadDICEkJjdUgMiSSSHVtYW5vaWREaWVk')] = humanoid.Died:Connect(function()
            if collisionClone then
                collisionClone:Destroy()
            end
        end)
    end

    rootPart = character:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VqJACgZJgmjurXIVvwGWgwTCkfJTzntFNXdtXOGPNEOmQgLJRXPNwAHSHVtYW5vaWRSb290UGFydA=='), 3)
    if rootPart then
        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('mvuqYkTudooRSwrZPEJRPunaNSdITLlsLspAriAlCHhIJhGhsGnglSkTm9BY2NlbGVyYXRpb24=')].Value then
            rootPart.CustomPhysicalProperties = PhysicalProperties.new(100, 0, 0, 0, 0)
        end

        if isFools then
            rootPart:GetPropertyChangedSignal(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('bnWftenDDIuCmFPkjSSIcoTlQCAYWbNpybUvfymqHqcHtAeKzIuBquWQW5jaG9yZWQ=')):Connect(function()
                if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('irTQORlkeJBJcmfBvbTpOkLoVlpbzJwCzdFpHffMAezNmWjvznefAILQW50aUFuY2hvcg==')].Value then
                    rootPart.Anchored = false
                end
            end)
        end
        
    end

    collision = character:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CCAWnEbEuywzjrXOMjweuyqPbaiNjyBNHBVTyZnGqgbHrkOJfYqNDdzQ29sbGlzaW9u'), 3)
    if collision then
        collisionClone = collision:Clone()
        collisionClone.CanCollide = false
        collisionClone.Massless = true
        collisionClone.Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uqxkkXgdhMGWjsTxMZRoQhPpyOFOBfLEYlSjYAyKkUNnqNxgpecLJtnQ29sbGlzaW9uQ2xvbmU=')
        if collisionClone:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ZaDuYqWGKobCxRBFyvTBBPornhdXqgTbBDlPiZJfjLzykPTHiinCuwNQ29sbGlzaW9uQ3JvdWNo')) then
            collisionClone.CollisionCrouch:Destroy()
        end
        collisionClone.Parent = character

        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('nJnNGMaoJfHtkwTKLHwvuqFQvnuVDcZDszFqWWjUtcYgnQwEUHSOswcVXBzaWRlRG93bg==')].Value then
            collision.Rotation = Vector3.new(collision.Rotation.X, collision.Rotation.Y, -90)
        end 
    end

    if reloading then
        Midnight:Notify(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OYbRLAIynBqBornPnAnDOmqJtVXiiNFapcVDCBticymAINCPaNRUzlaY2x1dGNoLmx1YSByZWxvYWRlZCBzdWNjZXNzZnVsbHkh'), 3)
    end
end

function setupOtherPlayerConnection(player)
    if not player then return warn(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('wLpLUwebBveZDgdNjhpBQxMHODCRsKcwRVyzotQdQdOUorLkFHSdaWiSW52YWxpZCBwbGF5ZXI=')) end

    if player.Character then
        if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HplrfKYWyOlJaLhLrcnOSJUKHoyJANKxCwxiweKKrhmzaZruceAqMTBRVNQV2hhdA==')].Value.Player then
            task.spawn(addPlayerEsp, player)
        end

        task.spawn(setupOtherCharacterConnection, player.Character)
    end

    connections[player.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JtzxPiAYztCfUEXdfVwLYuwdXDSroIysBLxyRxfAPYHLXckWAofqUxXQ2hhcmFjdGVyQWRkZWQ=')] = player.CharacterAdded:Connect(function(newCharacter)
        task.delay(0.1, function()
            if not newCharacter then return end

            if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('VyoPYnGfcEtLPgyDgjPAdgNaELdfjixqXzpKWeAvuFidenJnWrzYhmXRVNQV2hhdA==')].Value.Player then
                task.spawn(addPlayerEsp, player)
            end

            task.spawn(setupOtherCharacterConnection, newCharacter)
        end)
    end)
end

function setupOtherCharacterConnection(character)
    if not character then return warn(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('daOozWDIiqGCgqzqlLLIEjELoAldHnePmYBitCTEBgJzIjnrlnDkLJASW52YWxpZCBjaGFyYWN0ZXI=')) end

    connections[character.Name .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ddVrBFpnbAaqPgboNYJzpxwXxcHubSAxCyOkzawkmMzwOJOnHRUvDYeQ2hpbGRBZGRlZA==')] = character.ChildAdded:Connect(function(child)
        if child:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JAwfFPzfzSZOruoFSdnPsfFbZLDlPesMxpDZvQLOIQjSzlVJOzhGoxxVG9vbA==')) and child.Name:match(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('zNrXXuXPeOUMaaLrCePTwcwGVoiIvPtgJJPuinqwLpNUWDtYDZTPNSmTGlicmFyeUhpbnRQYXBlcg==')) then
            local code = table.concat(getPadlockCode(tool))
            local output, count = code:gsub(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TBbOFyVUVLCQiIgxUEQrAwsTBvfpWGYYbFwYHAjQhfzSpLAaVRqkUmvXw=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('McsfYjtawOguWnitNceVFGNYNaUxrdNbVAfKBStClwdlHIShUTriOeAeA=='))

            if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fcOplupyZVnaUnyXngNnvawLlMfVzxQgeQgWwwQiEFybvhLcBVtapWpQXV0b1BhZGxvY2s=')].Value and tonumber(code) then
                remotesFolder.PL:FireServer(code)
            end

            if count < 5 then
                if Flags[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XXTZmnnUNiUOJXmkmscPBaLCQNFJSUtwKXOvrbUkEIUtvVMxmrFjmzhTm90aWZ5UGFkbG9ja0NvZGU=')].Value then
                    Midnight:Notify(string.format(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XTbTENiITPpIfvaDgQxJBcmRbrSUYfxsnAfcdzqsKhUeDWFVmEIZFnqVGhlIHBhZGxvY2sgY29kZSBpczogJXM='), output))
                end
            end
        end
    end)
end

function getEntityName(entity)
    local entityName = entitiesTable.Names[entity.Name] or entity.Name

    if isFools and entityName == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KkqknnaufwdCwwvDqydvlpVjHWOoDQKQAtXgGtOrsUwyeSPZwarwAxaUnVzaA==') then
        entityName = entity.PrimaryPart.Name:gsub(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PZTpGFhePjmEHrYSMkiQgdoLurKkXEaPzeQxfApigkLEJkfVEgZcXqJTmV3'), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('OVWlPXkwwbVyqvqWDtwTLsMGEwlLyGjiiKahdzaGxPRdjwOjQAngFdJ'))
    end

    return entityName
end

function getEntitiesName()
    local names = {}

    for _, entity in pairs(entitiesTable.Entities) do
        local entityName = entitiesTable.Names[entity] or entity

        table.insert(names, entityName)
    end    

    return names
end

function getPadlockCode(paper)
    if paper and paper:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('xcBnptqEBtUOjeRcmnxayLIgKCtCKyapyVFuHWFeCfQzPmQDIBocsHgVUk=')) then
        local code = {}

        for _, image: ImageLabel in pairs(paper.UI:GetChildren()) do
            if image:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('qonxlJHrEWvcpRMxGQVdIltYltrgEMKsYVndWQXsfCGaUpXmshOlFUXSW1hZ2VMYWJlbA==')) and tonumber(image.Name) then
                code[image.ImageRectOffset.X .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JrihkxAuLYlpxVslGrlqrEglfjrcCDGKxUQcXsppihAlQUGbaAIlzzuIA==') .. image.ImageRectOffset.Y] = {tonumber(image.Name), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('AaFjxsrdFsyxMTWrWHSaKgYDDgIAnCQiRJVkWjvnVZPunidjmYRrbrjXw==')}
            end
        end

        for _, image in pairs(hints:GetChildren()) do
            if image.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('AaCLAZoPiGxpzKhZvWvSzEcIUXpbilitAwrwEqOhxetmPxZkaOWHALlSWNvbg==') then
                if code[image.ImageRectOffset.X .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HKSQuzXyTXBrKEUSfDSXUvftBLtiqQyAMLxuztTqRdnXtdfqXDpTpoQIA==') .. image.ImageRectOffset.Y] then
                    code[image.ImageRectOffset.X .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MvKtsEnzTKuecJOJBIWebyvAXoDbnJqwblkFThUensiavOdRZIFRnWpIA==') .. image.ImageRectOffset.Y][2] = image.TextLabel.Text
                end
            end
        end

        local normalizedCode = {}
        for _, num in pairs(code) do
            normalizedCode[num[1]] = num[2]
        end

        return normalizedCode
    end

    return {}
end

function getLookCFrame(cf)
    local cfPos = cf.Position
    return CFrame.new(cfPos, cfPos - (Vector3.new(camera.CFrame.Position.X, cfPos.Y, camera.CFrame.Position.Z) - cfPos).Unit)
end

