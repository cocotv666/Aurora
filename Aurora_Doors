local function saveConfig()
	local encrypt = game:GetService(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WnGHIgDxyGoztpfOdcLrVYHEblUeanRGPthGHcEedalSbZHdKPZsHfoSHR0cFNlcnZpY2U=')):JSONEncode(config)
	if isfile(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MZXYXtCPHNfacbBwARjCxKTPyqojbAzSsUifCpFEhWmZjiPAcaesJWrQXVyb3JhL2Rvb3JzY29uZmlnLmpzb24=')) then
		delfile(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MeCSKnaukgQPvclFzmTLidIBjMjsMSjlLGjRkblYggYLMfoWDPulGuPQXVyb3JhL2Rvb3JzY29uZmlnLmpzb24='))
	end
	writefile(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('sLDfWDwgEDPvHzfRHwztnQpMZlrmnMkiDiSKYObTYzFZeewYWWqJHQJQXVyb3JhL2Rvb3JzY29uZmlnLmpzb24='),encrypt)
end

local function loadConfig()
	local decrypt = game:GetService(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ADEVpFPnYUYUBcVCrnwgBHWgwaFQtcqtyYgUillUHgCgOtiJWOTrefbSHR0cFNlcnZpY2U=')):JSONDecode(readfile(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('mKbtpPtTjpjRqxJwtckYcPEPUuMybNhSwgCcMweorCLnyDimrVvUvIdQXVyb3JhL2Rvb3JzY29uZmlnLmpzb24=')))
	config = decrypt
end

if not isfile(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('sPoChPzghNAqNHRyDsJlTflsRJSVaZRBvSNnDyAFMfqYhKkcDQkhytWQXVyb3JhL2Rvb3JzY29uZmlnLmpzb24=')) then
	makefolder(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('GciFRiGkMeVgdyGCQIEKdlCeXqznmXoCYKSbLMTVUredDTYlOtcrmSjQXVyb3Jh'))
	saveConfig()
end

