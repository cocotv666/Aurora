})



local enemiesToDetect = {
    RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('GDIoFLfRKDpBsaoCgZsHmGmUPxQCOFbcDqiBgbbtZaqWPXSkFJmhNwxQmFja2Rvb3JSdXNo'), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('KujCuDJLNtwcHnrKjqgEZNEMkmECXXWJQEKWamvOqbnZCTJFiZpqHQSQmFja2Rvb3JMb29rbWFu'), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('gRnaLmWUmnCSrjfmJIcZlCAOSuBorpBVbtLIlQMsqRriIAQwuIiqkhAUnVzaE1vdmluZw=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MWzpVqXInBcUMQDoOzPuwMJEcJqTRABXkaLmYZpCIQxgIpHIzSyfvitQW1idXNoTW92aW5n'),
    RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HYbbbVppmiHBdRzDPNYzdIwVrmOKUMwTCvIsOdfRIABXEGtCvFvJqpVRXllcw=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('FsbSosZmErGEmqLrFXufEBxTkkgomZkyMKmAbpGxednJoLwHLKxgknLU2NyZWVjaA=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('zCJDEAMaWJihOkAiQXBoIeiNnMGzXnWhGKtEzgcDphGmpufsAfMmpDASGFsdA=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('WcQTpaAdWfnTYKrBpWefGKCTjkRuMEPZfKuCnNiRQXdzDzOGCjfeMFASmVmZlRoZUtpbGxlcg=='), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('MgdTvoXnUIaxoKUfXivwDMAxMDbrTKQlvouWrWkdbggbFjyyqZaRLMhQTYw'), RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('URcYRkobwdOUHmPCaStRPBrMtWJBxgmjYXruWdgnpnKryKqfxvoIRFdQTEyMA==')
}

local detectedEnemies = {} -- 用于存储已经发出通知的敌人
local monitoring = false -- 监测状态

-- 创建通知按钮
local enemynotification = Main.NewButton({
    Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('EENlgdfgoUSwKSfewAnPorRmkMitOYNIvHwQVqiAgCYHmPRiiltyCNgRW5lbXlOb3RpZmljYXRpb24='),
    Function = function(callback)
        if callback then
            if not monitoring then
                monitoring = true -- 开始监测
                task.spawn(function()
                    while monitoring do
                        for _, enemyName in ipairs(enemiesToDetect) do
                            local enemy = workspace:FindFirstChild(enemyName) -- 假设敌人模型在 workspace 中

                            -- 检测敌人
                            if enemy and not detectedEnemies[enemyName] then
                                detectedEnemies[enemyName] = true
                                sound = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CLhHQUuUgUQwKODzxDfkIzLpTxNXvtDqRLvBWsNuVhhVlMpISMDXLHYU291bmQ='), workspace)
                                sound.Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dXHCoHddASJIJhFcNDSuBfEtmAuDNAXPiadsZhWwphkIwvqfqndGkfRTm9vb29vb29vb29vb29vb290IG5vb29vb29vb29vb29vb29vb29vb29vb290')
                                sound.SoundId = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('YvhhMWGBzWNjcUtrUjnoBCQMCLbRHvESAgZpcONdnWxwShbuaalWIYfcmJ4YXNzZXRpZDovLzczODM1MjU3MTM=')
                                sound:Play()
                                CreateNotification2(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('UtKezaCjIRjDFFsXWIlyltMOVpsfxDgbbOoetgKBJpRQtcqcJONMiBhQXVyb3JhIHwgRW5lbXlOb3RpZmljYXRpb24='), 5, RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oYopMqNXVGRMyycYkhtUuoqJMdAeWwhBuzwZngnsvLkvOnecOrHkvLXRW5lbXkgZGV0ZWN0ZWQ6IA==') .. enemyName) -- 创建通知
                            elseif not enemy and detectedEnemies[enemyName] then
                                detectedEnemies[enemyName] = false
                                local sound = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('fmBIfIjNtbtWmotmnhEEKOgfKOJfUgeLQKnNFxrrcRLghZOlHndICxsU291bmQ='))
    sound.SoundId = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('gXTYtZyzCpLuoSLjQGxhEncqOzGOJRzbmCsxYKDDFUQMmhrmPEFcsgJcmJ4YXNzZXRpZDovLzQ1OTA2NTczOTE=')
    sound.Volume = 1
    sound.Parent = game.Players.LocalPlayer:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('eyjCBKYABIJeojdZIlyavuqisdBZmnEavhCzebEDfAiiJIPccdDyEUAUGxheWVyR3Vp'))
    sound:Play()
                                CreateNotification(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('TIOakWFEEDjIZodWrhkQnMAjJgPDmXJggYmNIQieLoAtiMslRMCBqTZQXVyb3JhIHwgRW5lbXlOb3RpZmljYXRpb24='), 5, RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('vjFruVZHNYTrCySlOlYmkKdobwUJfRGPGSDntPCSjuenlJGZnQOedhaRW5lbXkgZGlzYXBwZWFyZWQ6IA==') .. enemyName) -- 发出敌人已消失的通知
                            end
                        end
                        task.wait(1) -- 每隔一秒检查一次
                    end
                end)
            else
                monitoring = false -- 停止监测
            end
        end
    end,
})

AntiSeek = AntiEnemy.NewButton({
    Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('oMVysuRrYRqahiIPyOFDYkmWPnGiVOXxdGbiaIgjCrLYZgrnsjEMkDRQW50aVNlZWsgW0ZFXQ=='),
    Function = function(callback)
        if callback then
            repeat 
                task.wait()
                for _, room in pairs(workspace.CurrentRooms:GetChildren()) do
                    local triggerEventCollision = room:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('SYWMziRBYWwJyGeYsMypVcHcZffcegcIrbLHGelhObDoxcLrsREAmyWVHJpZ2dlckV2ZW50Q29sbGlzaW9u'))
                    if triggerEventCollision then
                        for _, part in pairs(triggerEventCollision:GetChildren()) do
                            part.CanTouch = false  -- 关闭触碰
                            part.CanCollide = false -- 关闭碰撞
                        end
                    end
                end                
            until (not AntiHalt.Enabled)
        end
    end,
})

AntiHalt = AntiEnemy.NewButton({
    Name = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('dHqomLdTqJryZZLDTQtHNWOybjeBXUChHxmQyUbRPuCfrmvNOxYOmubQW50aUhhbHQ='),
    Function = function(callback)
        if callback then
            repeat 
                task.wait()
                for _, instance in pairs(Workspace:GetChildren()) do
                    if instance.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('XTorWQuMgQZqaZjMnLshNrtkgvgjQJdQSweTWLKFDGpWucGZjKMaPJSSGFsdA==') then
                        instance:Destroy()
                    end
                end
            until (not AntiHalt.Enabled)
        end
    end,
})


local function MakeESP(name, highlightcolor, textcolor, additionalText)
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('svNjDARERjMduEwcFftNBcYVLTUxMpfAgJVMICqahPvOTZVDKgiAVwUTW9kZWw=')) and v.Name == name and not v:FindFirstChildOfClass(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CEjFjhVmXhCScUBEFVvfWpXgjFwFQaqbgNtjgJJNSBMbfzQsemCQXuASGlnaGxpZ2h0')) then
            -- 添加 Highlight
            local highlight = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('cMPjtSZHcFktxcoKVQcFpjibmiTaOWcOOXTnzPDqkoyXnvKLGAaISkeSGlnaGxpZ2h0'))
            highlight.FillColor = highlightcolor
            highlight.FillTransparency = 0.35
            highlight.Parent = v

            -- 创建 BillboardGui
            local billboardgui = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('uKdARwsVSVDXRarxzGIKtOsxxGGSPnVinDETiBeYgsJXhLstQqKoSAyQmlsbGJvYXJkR3Vp'))
            billboardgui.ExtentsOffset = Vector3.new(0, 0.5, 0)
            billboardgui.Size = UDim2.new(12, 0, 1, 0)
            billboardgui.AlwaysOnTop = true
            billboardgui.Parent = v

            local additionalLabel = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('bdvpAqqqwMufdCqZsXrMsbMRhoeaSpICUwGDQLzXGUdcYWmLxsJkuTLVGV4dExhYmVs'))
            additionalLabel.BackgroundTransparency = 1
            additionalLabel.TextScaled = true
            additionalLabel.TextColor3 = textcolor
            additionalLabel.Font = Enum.Font.Roboto
            additionalLabel.Size = UDim2.new(1, 0, 0.5, 0) -- 也设置高度
            additionalLabel.Position = UDim2.new(0, 0, 0.5, 0) -- 在下方
            additionalLabel.Text = additionalText -- 设置额外文本内容
            additionalLabel.Parent = billboardgui
        end
    end
end


local function MakeDoorESP(highlightcolor, textcolor)
    for _, model in pairs(workspace:GetDescendants()) do
        if model:IsA(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('puftLfpVGXCxkoqXLdPWLPgUexSGTioqwcFPGSTyOEKsnHFjaZyRzorTW9kZWw=')) and model.Name == RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('PsdthrxINUHVwkzwvVtRbZyPUAOnFEpLxzeYRbfVzSghskqDsbCYYJFRG9vcg==') then
            -- 检查模型内是否有名为 RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RIlrmpZqJZgIjeNPERthNTKRPYkpvnJvdeyVZujlhrsYJRfJkqluPLJRG9vcg==') 的部件
            local doorPart = model:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('JcurVxhlaJtIBomGJjtdnhEbLoCgdxNwIuPgetYyopWbHSuRClxunTPRG9vcg=='))
            if doorPart then
                -- 检查是否已存在 Highlight
                local highlight = model:FindFirstChildOfClass(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('SOXWFOGEIXkQMIcvhCfwJNXvDttwvaigmiWkRUnaoiUrhTHLCRpcrdqSGlnaGxpZ2h0'))
                if not highlight then
                    -- 添加 Highlight
                    highlight = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('CvCMGrqCexPZshuvmmDVrVJbmwLhSnbzPmtMtDbfZPromZUeetBKuioSGlnaGxpZ2h0'))
                    highlight.FillColor = highlightcolor
                    highlight.FillTransparency = 0.35
                    highlight.Parent = doorPart
                end
                
                -- 检查是否已存在 BillboardGui
                local billboardgui = model:FindFirstChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ETPBsKpLntwGLIsdWHUUwAiYkbgoxgnwlBApSIUjhaMbplCBYHAAIkZQmlsbGJvYXJkR3Vp'))
                if not billboardgui then
                    -- 创建 BillboardGui
                    billboardgui = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('UmWYQMbXOZMzdvZQGRnXJJggoRgbTfdJNSGsHbOXQqxWmckXGeerYGtQmlsbGJvYXJkR3Vp'))
                    billboardgui.ExtentsOffset = Vector3.new(0, 0.5, 0)
                    billboardgui.Size = UDim2.new(12, 0, 1, 0)
                    billboardgui.AlwaysOnTop = true
                    billboardgui.Parent = model

                    local textlabel = Instance.new(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('pYJXSdHljgZdqXJlxypZeouOtRcQdEcwKpQjGKpQFJouZdkSVrjDmCoVGV4dExhYmVs'))
                    textlabel.BackgroundTransparency = 1
                    textlabel.TextScaled = true
                    textlabel.TextColor3 = textcolor
                    textlabel.Font = Enum.Font.Roboto
                    textlabel.Size = UDim2.new(1, 0, 1, 0)

                    -- 使用 WaitForChild 以确保 Sign 和 Stinker 存在
                    local sign = model:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('qPjqBkbpveuxcTSzrJaUxgGGIOogHJDnTtTYAkTiPkosziNaYrvIVyoU2lnbg=='))
                    if sign then
                        local stinker = sign:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('HYuVeyMPyaUgALtYVlEPTytDjVqKbuwEPLnfYbfaDSCPuzTWqYNKIcnU3Rpbmtlcg=='))
                        if stinker then
                            textlabel.Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('IGCptkoKrJdAFnGMqrdUCUiHIWQhgoAsaLDuEDhpSTptRSQQQFIalMeRG9vciAo') .. stinker.Text .. RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('scLrRiblvuTokvqkXQmUsczOjAWxnAFImxEblMVVbISNrfHzxgHciTQKQ==')
                        else
                            textlabel.Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('juxTogDUzdZdCBOoAVrGqwRhyHFmuyGbWWJHYiGgdKdhuHnscxKkDegRG9vciAoTm8gU2lnbik=')
                        end
                    else
                        textlabel.Text = RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('APMbNPNlgctggWcUpUNVOTKxCxPFrKmwsgUDACSnnyJmdAgfruqptacRG9vciAoTm8gU2lnbik=')
                    end

                    textlabel.Parent = billboardgui
                end
            end
        end
    end
end

