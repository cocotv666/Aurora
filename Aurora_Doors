local remotesFolder = isFools and ReplicatedStorage:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('ygkyaAttZWVgTjmjoFvdfHuVvPDNvOTjzzjkcfkaFuBgBiHrYJmzLZkRW50aXR5SW5mbw==')) or ReplicatedStorage:WaitForChild(RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('RRSlbhrAiIhqjnRqwpdNHehqeZsjddUDLhnsLrxzEFDhjXvNiQUBWBLUmVtb3Rlc0ZvbGRlcg=='))

