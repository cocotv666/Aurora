		[RiGXcOQYOPutrYnWNDydnPXvLJuWjBVbabZT('hDcwNopHTOgpPlfrMRZxqeUzZmtMBHLMosRgRmdTRazEZNzbIgGvQZoTWFpbg==')] = Color3.fromRGB(0, 145, 17),
